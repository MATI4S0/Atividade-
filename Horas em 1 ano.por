programa
{
	
	funcao inicio()
	{
	inteiro hora = 8766
	cadeia resposta
		escreva("Você quer saber quantas horas tem em um ano?\n")
			leia(resposta)

		se(resposta == "Sim"){
			escreva("Em um ano tem ", hora, " horas")
		}
		senao{
			escreva("Não ligo pra sua resposta\nEm um ano tem ", hora, " horas")
		}
	}
}
