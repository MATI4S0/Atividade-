programa
{
	
	funcao inicio()
	{
	inteiro decada = 10
	inteiro dias = decada *365
	inteiro minutos_em_dia = 24 * 60
	inteiro minutos_decada = dias * minutos_em_dia




	
	cadeia resposta 
		escreva("Vamos ver quantos minutos em uma década?\n")
			leia(resposta)

		se(resposta == "Sim"){
			escreva("Em uma década tem ", minutos_decada, " minutos")

		}
		senao{
			escreva("Você é chato")
		}
	}
	
}
